		"php-di/php-di": "^6.0",
		"slim/slim" : "^3.0",
		"jsonrpc/jsonrpc": "^1.0",
		"vlucas/phpdotenv" : "^2.4.0",
		"semsol/arc2" : "~2.3.1",
		"cloudobjects/rdfutilities" : "dev-master",
		"tuupola/slim-basic-auth": "^3.2",
		"dflydev/fig-cookies": "^1.0",
		"doctrine/collections": "^1.5",
		"defuse/php-encryption": "^2.2",
		"webmozart/assert": "^1.3",
		"ramsey/uuid": "^3.8",
		"jenssegers/agent": "^2.6"
