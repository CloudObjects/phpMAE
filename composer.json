{
	"name" : "cloudobjects/phpmae",
	"description" : "The PHP Micro API Engine.",
	"keywords": ["cloudobjects", "serverless", "faas", "microapi", "sandbox"],
	"homepage": "https://github.com/CloudObjects/phpMAE",
